{
    "name": "hoyvoy/laravel-subdomain-localization",
    "description": "Subdomain localization support for Laravel",
    "keywords": ["subdomain", "localization", "laravel", "php"],
    "homepage": "https://github.com/hoyvoy/laravel-subdomain-localization",
    "license": "MIT",
    "authors": [
        {
            "name": "Mario Aguilar Alves",
            "email": "maguilar@hoyvoy.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "illuminate/support": "^6.0",
        "illuminate/container": "^6.0",
        "illuminate/events": "^6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "orchestra/testbench": "3.6.*",
        "orchestra/testbench-browser-kit": "3.6.*"
    },
    "autoload": {
        "psr-4": {
            "Hoyvoy\\Localization\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Hoyvoy\\Tests\\" : "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Hoyvoy\\Localization\\LocalizationServiceProvider"
            ]
        }
    }
}
